{
  "name": "Marketplace-v2-API",
  "version": "0.0.1",
  "description": "API endpoint(s) to allow dealers to claim their dealership from vinaudit, verify their dealer email, and onboard to Ultralead by saving record to MongoDB. TypeScript NodeJS app using ExpressJS and MongoDB.",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "dev": "tsc && nodemon src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "deploy-dev": "tsc && serverless deploy --config serverless-dev.yml --org=mdunham --app=carbeeza-ultralead-onboarding-system",
    "deploy-prod": "tsc && serverless deploy --config serverless-prod.yml --org=mdunham --app=carbeeza-ultralead-onboarding-system",
    "deploy-local": "tsc && serverless offline --config serverless-dev.yml start"
  },
  "keywords": [],
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.1.3",
    "@types/nodemailer": "^6.4.8",
    "@types/random-string": "^0.0.28",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "serverless-offline": "^12.0.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "geoip-country": "^4.2.20",
    "imap": "^0.8.19",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^5.5.0",
    "mongoose": "^7.2.2",
    "nodemailer": "^6.9.3",
    "random-string": "^0.2.0",
    "serverless-http": "^3.2.0",
    "typescript": "^5.0.4"
  }
}
